generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(uuid())
  name      String
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  deletedAt DateTime?
  phone     String
}

model AutoCheckUser {
  id          String    @id @default(uuid())
  name        String
  plate       String    @unique
  createdAt   DateTime  @default(now())
  presentedAt DateTime?
  photo       String?
}
